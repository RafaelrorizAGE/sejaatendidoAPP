<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#5260ff</color>
    <color name="colorPrimary">#3880ff</color>
    <color name="colorPrimaryDark">#3171e0</color>
    <color name="ic_launcher_background">#F5F4F4</color>
    <string name="app_name">Appoint Sync Mobile</string>
    <string name="custom_url_scheme">com.meuapp.mobile</string>
    <string name="package_name">com.meuapp.mobile</string>
    <string name="title_activity_main">Appoint Sync Mobile</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:background">@android:color/white</item>
    </style>
    <style name="AppTheme.NoActionBarLaunch" parent="AppTheme.NoActionBar">
        <item name="android:background">@drawable/splash</item>
    </style>
</resources>